<%
	var posts = public.about._data;
%>

<section class="minc-inner-lead">
	<div class="row">
		<div class="col-sm-12">
			<img class="minc-inner-logo" src="<%= site_url %>/assets/svg/MINC_Logo.svg">
		</div>
	</div>
</section>

<nav class="navbar-minc-about" data-spy="affix" data-offset-top="305">
	<div class="container">
    <!-- Brand and toggle get grouped for better mobile display -->
    <div class="navbar-header">
      <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#minc-about-nav" aria-expanded="false">
        <span class="sr-only">Toggle navigation</span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </button>
    </div>

    <!-- Collect the nav links, forms, and other content for toggling -->
    <div class="collapse navbar-collapse" id="minc-about-nav">
      <ul class="nav navbar-nav">
				<% for(var i = 0, x = posts.length; i < x; i++){ %>
        <li class=""><a href="#section-<%= posts[i].classname %>"><%= posts[i].menu %></a></li>
				<% } %>
      </ul>
    </div><!-- /.navbar-collapse -->
  </div><!-- /.container-fluid -->

</nav>

<%
  for(var i = 0, x = 3; i < x; i++){
%>

<section id="section-<%= posts[i].classname %>" class="minc-about-section <%= posts[i].classname %>">
	<h3 class="minc-about-section-title"><%= posts[i].title %></h3>
	<div class="container">
		<div class="row">
			<div class="col-sm-10 col-sm-offset-1">
				<div class="section-content">
					<%- partial(posts[i].file) %>
				</div>
			</div>
		</div>
	</div>
</section>

<% } %>

<section id="section-<%= posts[3].classname %>" class="minc-about-section <%= posts[3].classname %>">
	<h3 class="minc-about-section-title"><%= posts[3].title %></h3>
	<div class="container">
		<div class="row">
			<div class="col-sm-10 col-sm-offset-1">
				<div class="section-content">
					<%- partial(posts[3].file) %>
					<div class="row">
					<%
					  var judges = public.judges._data;
					  for(var i = 0, x = judges.length; i < x; i++){
					%>
						<div class="col-sm-4">
							<%= judges[i].name %>
							<%- partial('../judges/' + judges[i].file) %>

						</div>
					<% } %>
					</div>
				</div>
			</div>
		</div>
	</div>
</section>
